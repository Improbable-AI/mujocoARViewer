<mujoco model="ur5e scene">
  <include file="ur5e.xml"/>

  <statistic center="0.3 0 0.4" extent="0.8"/>

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.1 0.1 0.1" specular="0 0 0"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <global azimuth="120" elevation="-20"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
  </asset>

  <worldbody>
    <light pos="0 0 1.5" dir="0 0 -1" directional="true"/>
    <geom name="floor" size="0 0 0.05" type="plane" material="groundplane"/>
    <body name="target" pos="0.3 0 .3" quat="0 1 0 0" mocap="true">
      <geom type="box" size=".01 .01 .01" contype="0" conaffinity="0" rgba=".6 .3 .3 .5"/>
      <site type="sphere" size="0.01" rgba="0 0 1 1" group="1"/>
    </body>
    <body name="cube1">
      <freejoint/>
      <geom type="box" size="0.03 0.03 0.03" contype="1" conaffinity="1" rgba=".6 .0 .0 1." mass="0.01" condim="4" friction="1.0 1.0 1.0"/>
      <body name="cube1_grasp" pos="0.0 0.0 0.0" euler="3.1416 0 0">
      <!-- geoms for x,y,z axis as cylinder, with RGB color -->
        <geom type="cylinder" size="0.003 0.02" pos="0.02 0 0" rgba="1 0 0 1" euler="0 1.5708 0" class="visual"/>
        <geom type="cylinder" size="0.003 0.02" pos="0 0.02 0" rgba="0 1 0 1" euler="1.5708 0 0" class="visual"/>
        <geom type="cylinder" size="0.003 0.02" pos="0 0 0.02" rgba="0 0 1 1"  euler="0 0 0" class="visual"/>
      </body>
    </body>
    <body name="cube2">
      <freejoint/>
      <geom type="box" size="0.03 0.03 0.03" contype="1" conaffinity="1" rgba=".3 .6 .3 1." mass="0.01" condim="4" friction="1.0 1.0 1.0"/>
      <body name="cube2_grasp" pos="0.0 0.0 0.0" euler="3.1416 0 0">
      <!-- geoms for x,y,z axis as cylinder, with RGB color -->
        <geom type="cylinder" size="0.003 0.02" pos="0.02 0 0" rgba="1 0 0 1" euler="0 1.5708 0" class="visual"/>
        <geom type="cylinder" size="0.003 0.02" pos="0 0.02 0" rgba="0 1 0 1" euler="1.5708 0 0" class="visual"/>
        <geom type="cylinder" size="0.003 0.02" pos="0 0 0.02" rgba="0 0 1 1"  euler="0 0 0" class="visual"/>
      </body>
    </body>
  </worldbody>
</mujoco>
